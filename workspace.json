{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "cra-to-nx-bugs",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "cra-to-nx-bugs": {
      "root": "apps/cra-to-nx-bugs",
      "sourceRoot": "apps/cra-to-nx-bugs/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired build",
            "cwd": "apps/cra-to-nx-bugs",
            "outputPath": "dist/apps/cra-to-nx-bugs"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired start",
            "cwd": "apps/cra-to-nx-bugs"
          }
        },
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/eslint src/**/*.tsx src/**/*.ts",
            "cwd": "apps/cra-to-nx-bugs"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/.bin/react-app-rewired test --watchAll=false",
            "cwd": "apps/cra-to-nx-bugs"
          }
        }
      }
    },
    "cra-to-nx-bugs-e2e": {
      "root": "apps/cra-to-nx-bugs-e2e",
      "sourceRoot": "apps/cra-to-nx-bugs-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx e2e-serve cra-to-nx-bugs-e2e",
              "nx e2e-run cra-to-nx-bugs-e2e"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/cra-to-nx-bugs-e2e/**/*.{js,ts}"
            ]
          }
        },
        "e2e-run": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/cra-to-nx-bugs-e2e/cypress.json",
            "tsConfig": "apps/cra-to-nx-bugs-e2e/tsconfig.e2e.json",
            "baseUrl": "http://localhost:3000"
          }
        },
        "e2e-serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx serve cra-to-nx-bugs"
            ],
            "readyWhen": "can now view"
          }
        }
      }
    }
  }
}